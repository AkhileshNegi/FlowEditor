{
  "name": "@nyaruka/flow-editor",
  "license": "AGPL-3.0",
  "repository": "git://github.com/nyaruka/floweditor.git",
  "version": "1.4.19",
  "description": "'Standalone flow editing tool designed for use within the RapidPro suite of messaging tools but can be adopted for use outside of that ecosystem.'",
  "browser": "umd/flow-editor.min.js",
  "unpkg": "umd/flow-editor.min.js",
  "author": "Nyaruka <code@nyaruka.com>",
  "files": [
    "umd"
  ],
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "lambda:serve": "netlify-lambda serve lambda-src -p 8888",
    "lambda:build": "netlify-lambda build lambda-src",
    "netlify": "npm run build && npm run lambda:build",
    "version": "npm run build:umd && auto-changelog -p && git add CHANGELOG.md",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "lcov",
      "json"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@types/auto-bind": "2.1.0",
    "@types/common-tags": "1.4.0",
    "@types/core-js": "2.5.0",
    "@types/dateformat": "3.0.0",
    "@types/enzyme": "3.1.13",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "24.0.13",
    "@types/react-addons-shallow-compare": "0.14.22",
    "@types/react-dom": "16.0.7",
    "@types/react-modal": "3.8.1",
    "@types/react-redux": "5.0.15",
    "@types/react-select": "2.0.17",
    "@types/redux-mock-store": "1.0.0",
    "@types/textarea-caret": "3.0.0",
    "@types/uniqid": "4.1.2",
    "@types/uuid": "3.4.3",
    "@types/webpack-env": "1.13.6",
    "ajv": "6.5.2",
    "array-move": "2.1.0",

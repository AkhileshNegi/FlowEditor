{
  "name": "flow-editor",
  "license": "AGPL-3.0",
  "repository": "git://github.com/nyaruka/floweditor.git",
  "version": "1.1.0",
  "description": "'Standalone flow editing tool designed for use within the RapidPro suite of messaging tools but can be adopted for use outside of that ecosystem.'",
  "main": "app.tsx",
  "author": "Nyaruka <code@nyaruka.com>",
  "scripts": {
    "test": "jest --maxWorkers=4 --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:local": "jest",
    "test:watch": "npm run test:local -- --watch",
    "test:coverage": "npm run test:local -- --coverage",
    "test:update": "npm run test:local -- --updateSnapshot",
    "start": "webpack-dev-server --env.NODE_ENV=dev --progress --colors",
    "lint:es": "eslint '*/**/*.js'",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "build:dev": "webpack --env.NODE_ENV=dev --progress --profile --colors",
    "build:prod": "webpack --env.NODE_ENV=prod --progress --profile --colors",
    "build:both": "npm run build:dev && npm run build:prod",
    "bundleStats:dev": "webpack --env.NODE_ENV=dev --profile --json > stats.json",
    "bundleStats:dev:windows": "webpack --env.NODE_ENV=dev --profile --json | Out-file 'stats.json' -Encoding OEM",
    "bundleStats:prod": "webpack --env.NODE_ENV=prod --profile --json > stats.json",
    "bundleStats:prod:windows": "webpack --env.NODE_ENV=prod --profile --json | Out-file 'stats.json' -Encoding OEM",
    "analyze": "webpack-bundle-analyzer stats.json ./dist -m 'server'",
    "runAnalyzer:dev": "npm run bundleStats:dev && npm run analyze",
    "runAnalyzer:dev:windows": "npm run bundleStats:dev:windows && npm run analyze",
    "runAnalyzer:prod": "npm run bundleStats:prod && npm run analyze",
    "runAnalyzer:prod:windows": "npm run bundleStats:prod:windows && npm run analyze",
    "preprettify": "prettier --debug-check '{,!(node_modules)/**/}*.@(ts|tsx)'",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'",
    "preversion": "npm run test:local",
    "version": "npm run build:prod",
    "postversion": "git push && git push --tags && npm publish"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.41",
    "@types/enzyme": "^2.8.0",
    "@types/enzyme-adapter-react-15": "^1.0.0",
    "@types/immutability-helper": "^2.0.15",
    "@types/jest": "^21.1.4",
    "@types/jquery": "^2.0.40",
    "@types/node": "^7.0.5",
    "@types/prop-types": "^15.5.2",
    "@types/query-string": "^4.3.1",
    "@types/react": "^15.0.14",
    "@types/react-addons-shallow-compare": "^0.14.19",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-dnd": "^2.0.32",
    "@types/react-dnd-html5-backend": "^2.1.7",
    "@types/react-dom": "^0.14.23",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-measure": "^0.4.6",
    "@types/react-modal": "^1.6.6",
    "@types/react-select": "^1.0.46",
    "@types/uniqid": "^4.1.2",
    "@types/url-join": "^0.8.2",
    "@types/uuid": "^2.0.30",
    "@types/webpack-env": "^1.13.1",
    "@types/xregexp": "^3.0.29",
    "awesome-typescript-loader": "^3.1.2",
    "axios": "^0.15.3",
    "compression-webpack-plugin": "^0.4.0",
    "coveralls": "^3.0.0",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^0.26.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.12.1",
    "eslint-config-xo-space": "^0.17.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "get-input-selection": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "immutability-helper": "^2.1.2",
    "jest": "^21.2.1",
    "jquery": "^3.1.1",
    "jsplumb": "^2.4.2",
    "local-storage": "^1.4.2",
    "nock": "^9.0.28",
    "node-sass": "^4.5.0",
    "prettier": "^1.7.4",
    "promise-polyfill": "^6.0.2",
    "prop-types": "^15.6.0",
    "query-string": "^4.3.4",
    "react": "^15.5.1",
    "react-addons-shallow-compare": "^15.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.5.1",
    "react-flip-move": "^2.9.14",
    "react-hot-loader": "^3.1.1",
    "react-measure": "^2.0.0",
    "react-modal": "^1.7.5",
    "react-modal-dialog": "^4.0.7",
    "react-select": "^1.0.0-rc.10",
    "react-test-renderer": "^15.5.1",
    "react-tether": "^0.5.7",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.1.6",
    "split-sms": "^0.1.7",
    "style-loader": "^0.13.2",
    "textarea-caret": "^3.0.2",
    "ts-jest": "^21.1.3",
    "ts-loader": "^2.0.1",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "typings-for-css-modules-loader": "^1.5.0",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "uniqid": "^4.1.1",
    "url-join": "^2.0.2",
    "url-loader": "^0.5.8",
    "uuid": "^3.0.1",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  }
}
